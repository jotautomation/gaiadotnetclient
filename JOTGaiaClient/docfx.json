{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "client.cs",
            "Application.cs"
          ],
          "cwd": ".",
          "exclude": [
            "**/obj/**",
            "**/bin/**",
            "../docs/**"
          ]
        }
      ],
      "dest": "obj/api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml"
        ],
        "cwd": "obj"
      },
      {
        "files": [
          "api/*.md",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "obj/**",
          "../docs/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "logo-jot-color.png",
          "jot_favicon.ico"
        ],
        "exclude": [
          "obj/**",
          "../docs/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "../docs/**"
        ]
      }
    ],
    "dest": "../docs",
    "template": [
      "default",
      "main.css"
    ],
    "globalMetadata": {
      "_appTitle": ".NET Gaia Client",
      "_appFooter": "Copyright © 2019 JOT Automation Ltd.",
      "_appFaviconPath": "jot_favicon.ico",
      "_disableBreadcrumb": true,
      "_disableAffix": true,
      "_disableContribution": true
    }
  }
}